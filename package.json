{
  "name": "APWebpack",
  "version": "2.0.0",
  "description": "WebPack: HTML, CSS, SCSS, JS, Img, Fonts",
  "main": "bundle.js",
  "scripts": {
    "build": "npm run clean -s && npm run buildpost -s",
    "buildpost": "node_modules/.bin/webpack --progress",
    "clean": "shx rm -rf dist",
    "webpack": "webpack",
    "sass": "node-sass src/app/css/style.scss dist/index.css --output-style compressed",
    "lint": "eslint src",
    "dev": "webpack-dashboard -- webpack-dev-server",
    "test": "set NODE_ENV=test && jest",
    "start": "serve -s dist",
    "devStart": "node_modules/.bin/webpack-dev-server --history-api-fallback --inline --progress"
  },
  "author": "Alexander Panov",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "^10.4.16",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-polyfill": "6.26.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^6.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-jest": "^23.6.0",
    "file-loader": "^5.0.2",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.2.2",
    "minimist": "^1.2.6",
    "node-sass": "^9.0.0",
    "postcss-cli": "^10.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^7.5.2",
    "semantic-release": "^25.0.2",
    "serialize-javascript": "^3.0.0",
    "serve": "^14.2.5",
    "shx": "^0.3.2",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^5.1.4",
    "webpack-dashboard": "^3.3.8",
    "webpack-dev-server": "^5.2.1"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Jest Tests",
          "outputPath": "test-report/test-log.html",
          "includeFailureMsg": true,
          "theme": "defaultTheme",
          "styleOverridePath": "test/teststyle.css"
        }
      ]
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "dotenv": "^8.2.0",
    "eslint": "^9.39.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^5.5.1",
    "image-webpack-loader": "^8.1.0",
    "jest-html-reporter": "^2.8.2",
    "mini-css-extract-plugin": "^0.9.0",
    "sass-loader": "^8.0.2"
  }
}
